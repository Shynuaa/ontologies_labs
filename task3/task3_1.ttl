@prefix : <http://www.semanticweb.org/aleks/ontologies/2025/9/untitled-ontology-8/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/aleks/ontologies/2025/9/untitled-ontology-8/> .

<http://www.semanticweb.org/aleks/ontologies/2025/9/untitled-ontology-8/> rdf:type owl:Ontology ;
    rdfs:label "Банковская онтология для кредитного скоринга" ;
    rdfs:comment "Расширенная онтология для автоматизации процесса одобрения кредитных заявок" .

#################################################################
#    Classes
#################################################################

###  Классы предметной области

:Заемщик rdf:type owl:Class ;
    rdfs:label "Заемщик" ;
    rdfs:comment "Физическое или юридическое лицо, подающее кредитную заявку" .

:КредитнаяЗаявка rdf:type owl:Class ;
    rdfs:label "Кредитная заявка" ;
    rdfs:comment "Запрос на получение кредита от заемщика" .

:ОдобреннаяЗаявка rdf:type owl:Class ;
    rdfs:label "Одобренная заявка" ;
    rdfs:comment "Кредитная заявка, удовлетворяющая критериям одобрения" ;
    rdfs:subClassOf :КредитнаяЗаявка .

:ОтклоненнаяЗаявка rdf:type owl:Class ;
    rdfs:label "Отклоненная заявка" ;
    rdfs:comment "Кредитная заявка, не удовлетворяющая критериям одобрения" ;
    rdfs:subClassOf :КредитнаяЗаявка .

:КредитныйПродукт rdf:type owl:Class ;
    rdfs:label "Кредитный продукт" ;
    rdfs:comment "Тип кредитного продукта (ипотека, автокредит, потребительский кредит)" .

:Банк rdf:type owl:Class ;
    rdfs:label "Банк" ;
    rdfs:comment "Кредитная организация" .

:КредитнаяИстория rdf:type owl:Class ;
    rdfs:label "Кредитная история" ;
    rdfs:comment "История кредитных отношений заемщика" .

#################################################################
#    Object Properties
#################################################################

###  Объектные свойства (связи между объектами)

:поданаЗаемщиком rdf:type owl:ObjectProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range :Заемщик ;
    rdfs:label "подана заемщиком" .

:имеетКредитнуюИсторию rdf:type owl:ObjectProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range :КредитнаяИстория ;
    rdfs:label "имеет кредитную историю" .

:относитсяКПродукту rdf:type owl:ObjectProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range :КредитныйПродукт ;
    rdfs:label "относится к продукту" .

:обрабатываетсяБанком rdf:type owl:ObjectProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range :Банк ;
    rdfs:label "обрабатывается банком" .

#################################################################
#    Data Properties
#################################################################

###  Свойства-данные для заемщика

:имеетКредитныйРейтинг rdf:type owl:DatatypeProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range xsd:integer ;
    rdfs:label "имеет кредитный рейтинг" .

:имеетТекущиеПросрочки rdf:type owl:DatatypeProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range xsd:boolean ;
    rdfs:label "имеет текущие просрочки" .

:имеетЕжемесячныйДоход rdf:type owl:DatatypeProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range xsd:decimal ;
    rdfs:label "имеет ежемесячный доход" .

:имеетВозраст rdf:type owl:DatatypeProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range xsd:integer ;
    rdfs:label "имеет возраст" .

:имеетСтажРаботы rdf:type owl:DatatypeProperty ;
    rdfs:domain :Заемщик ;
    rdfs:range xsd:integer ;
    rdfs:label "имеет стаж работы" .

###  Свойства-данные для кредитной заявки

:имеетСуммуКредита rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range xsd:decimal ;
    rdfs:label "имеет сумму кредита" .

:имеетСрокКредита rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range xsd:integer ;
    rdfs:label "имеет срок кредита" .

:имеетСтавку rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range xsd:decimal ;
    rdfs:label "имеет ставку" .

:имеетДатуПодачи rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяЗаявка ;
    rdfs:range xsd:date ;
    rdfs:label "имеет дату подачи" .

###  Свойства-данные для кредитной истории

:имеетКоличествоКредитов rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяИстория ;
    rdfs:range xsd:integer ;
    rdfs:label "имеет количество кредитов" .

:имеетПросрочкиВИстории rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяИстория ;
    rdfs:range xsd:boolean ;
    rdfs:label "имеет просрочки в истории" .

:имеетОбщуюЗадолженность rdf:type owl:DatatypeProperty ;
    rdfs:domain :КредитнаяИстория ;
    rdfs:range xsd:decimal ;
    rdfs:label "имеет общую задолженность" .

#################################################################
#    Individuals
#################################################################

