    :имеетОбщуюЗадолженность "150000.00"^^xsd:decimal .

:История_Сидоровой rdf:type owl:NamedIndividual , :КредитнаяИстория ;
    :имеетКоличествоКредитов "1"^^xsd:integer ;
    :имеетПросрочкиВИстории "false"^^xsd:boolean ;
    :имеетОбщуюЗадолженность "50000.00"^^xsd:decimal .

###  Кредитные продукты

:Ипотека rdf:type owl:NamedIndividual , :КредитныйПродукт ;
    rdfs:label "Ипотечный кредит" .

:Автокредит rdf:type owl:NamedIndividual , :КредитныйПродукт ;
    rdfs:label "Автокредит" .

:ПотребительскийКредит rdf:type owl:NamedIndividual , :КредитныйПродукт ;
    rdfs:label "Потребительский кредит" .

###  Банк

:АльфаБанк rdf:type owl:NamedIndividual , :Банк ;
    rdfs:label "Альфа-Банк" .

